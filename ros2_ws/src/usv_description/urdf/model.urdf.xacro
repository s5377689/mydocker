<?xml version="1.0"?>
<robot name="$(arg usv_name)" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <!-- Arguments -->
    <xacro:arg name="fdm_port_in" default="9002"/>

    <!-- Model -->
    <xacro:include
        filename="$(find usv_description)/urdf/common_properties.xacro"/>
    <xacro:include
        filename="$(find usv_description)/urdf/model_base.xacro"/>
    <!-- Gazebo -->
    <xacro:include
        filename="$(find usv_description)/urdf/gazebo.xacro"/>
</robot>